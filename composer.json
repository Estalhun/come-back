{
  "name": "sanzeeb3/come-back",
  "description": "Send email notification to inactive customers. Bring them back.",
  "homepage": "https://github.com/sanzeeb3/come-back",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=5.6",
    "composer/installers": "1.7.0",
     "woocommerce/action-scheduler": "^3.1.6"
  },
   "autoload": {
    "psr-4": {
        "ComeBack\\": "src/"
    }
  },
  "scripts": {
    "phpcs": [
      "phpcs -s -p"
    ],
    "phpcs-pre-commit": [
      "phpcs -s -p -n"
    ],
    "phpcbf": [
      "phpcbf -p"
    ]
  }
}
